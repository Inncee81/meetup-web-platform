  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "dependencies": {
    "mwp-auth-plugin": ">=0.0.1",
    "react-side-effect": "1.1.3"
  },
  "devDependencies": {
    "mwp-test-utils": ">=0.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
