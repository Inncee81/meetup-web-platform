{
  "name": "mwp-router",
  "version": "0.0.1",
  "description": "React routing tools for MWP apps",
  "engineStrict": true,
  "engines": {
    "node": ">=8.1.0"
  },
  "main": "./lib/index.js",
  "scripts": {
