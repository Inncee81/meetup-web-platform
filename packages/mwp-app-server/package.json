{
  "name": "mwp-app-server",
  "description": "Hapi server startup module for MWP applications",
  "engineStrict": true,
  "engines": {
    "node": ">=8.1.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx --source-maps",
    "flow": "flow",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test": "yarn flow && yarn run test:unit -- --silent --coverage",
    "test:unit": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "yarn test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "dependencies": {
    "electrode-csrf-jwt": "1.2.1",
    "hapi": "16.5.2",
    "hapi-pino": "1.7.0",
    "inert": "4.2.1",
    "mwp-api-proxy-plugin": "6.0.1609-beta",
    "mwp-app-route-plugin": "6.0.1609-beta",
    "mwp-auth-plugin": "6.0.1609-beta",
    "mwp-cli": "1.1.123",
    "mwp-core": "6.0.1609-beta",
    "mwp-language-plugin": "6.0.1609-beta",
    "mwp-sw-plugin": "6.0.1609-beta",
    "mwp-tracking-plugin": "6.0.1609-beta",
    "pino": "4.7.1",
    "spdy": "3.4.7",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "convict": "4.0.0",
    "flow-bin": "0.49.1",
    "jest": "20.0.4"
  }
}