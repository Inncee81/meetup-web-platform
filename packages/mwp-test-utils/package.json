{
  "name": "mwp-test-utils",
  "version": "0.0.1",
  "description": "Helpers for running unit tests in MWP apps",
  "engineStrict": true,
  "engines": {
    "node": ">=8.1.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx --source-maps",
    "flow": "flow",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test": "yarn run test:unit",
    "test:unit": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "yarn test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "peerDependencies": {
    "mwp-cli": "^1.1.268",
    "mwp-router": ">=0.0.1",
    "rxjs": "^5.4.3"
  },
  "dependencies": {
    "electrode-csrf-jwt": "1.2.1",
    "hapi": "16.5.2",
    "inert": "4.2.1",
    "meetup-web-mocks": "1.0.217",
    "mwp-core": ">=0.0.1",
    "mwp-logger-plugin": ">=0.0.1",
    "mwp-store": ">=0.0.1",
    "react-helmet": "5.2.0",
    "redux": "3.7.2",
    "tough-cookie": "2.3.2"
  },
  "devDependencies": {}
}