{
  "collectCoverageFrom": [
    "packages/**/*.{js,jsx}",
    "!**/{node_modules,lib,coverage,scripts}/**"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json"
  ],
  "setupFiles": [
    "<rootDir>/tests/util/setupTest.js"
  ],
  "testEnvironment": "node",
  "testRegex": "src\/.*\\.test\\.jsx?$"
}